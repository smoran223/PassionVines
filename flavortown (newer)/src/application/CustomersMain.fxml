<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="900.0" prefWidth="700.0" styleClass="root" stylesheets="@CustomersMain.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CustomersMainController">
   <children>
      <Button fx:id="returnButton" layoutX="585.0" layoutY="860.0" mnemonicParsing="false" onAction="#returnToMain" />
      <Button fx:id="addButton" layoutX="282.5" layoutY="862.5" mnemonicParsing="false" onAction="#addCustomers" text="Add Customers" />
      <TextField layoutX="25.0" layoutY="186.0" prefHeight="31.0" prefWidth="550.0" promptText="Search..." />
      <ScrollPane fx:id="scrollPane" fitToWidth="true" hbarPolicy="NEVER" layoutX="24.0" layoutY="285.0" prefHeight="546.0" prefWidth="654.0" vbarPolicy="ALWAYS">
         <content>
            <ListView fx:id="scrollPaneListView" prefHeight="1000.0" prefWidth="455.0" />
         </content>
      </ScrollPane>
      <Button fx:id="displayButton" layoutX="597.0" layoutY="186.0" mnemonicParsing="false" onAction="#display" text="Display" />
      <Label fx:id="nameLabel" layoutX="75.0" layoutY="235.0" text="Name" />
      <Label fx:id="emailLabel" layoutX="275.0" layoutY="235.0" text="Email" />
      <Label fx:id="phoneLabel" layoutX="475.0" layoutY="235.0" text="Phone" />
   </children>
</AnchorPane>
